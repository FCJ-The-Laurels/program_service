			"item": [
				{
					"name": "Who Am I",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", () => pm.response.to.have.status(200));",
									"pm.test(\"Response shows correct principal and roles\", () => {",
									"    const res = pm.response.json();",
									"    pm.expect(res.principal).to.equal(pm.environment.get(\"userId\"));",
									"    pm.expect(res.roles[0].authority).to.equal(\"ROLE_USER\");",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "X-User-Id",
								"value": "{{userId}}"
							},
							{
								"key": "X-User-Role",
								"value": "USER"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/debug/whoami",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"debug",
								"whoami"
							]
						},
						"description": "Checks the current authenticated user context. Only available in 'dev' profile."
